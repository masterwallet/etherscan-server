{
  "name": "etherscan-server",
  "version": "0.0.1",
  "description": "EtherScan Server - emulating some functions of etherscan.io API for testing",
  "main": "server.js",
  "scripts": {
    "start": "cross-env DEBUG=*,-follow-redirects nodemon server.js --cors",
    "init:schema": "sudo mysql -u root etherscan -e \"\\. db/schema.sql\" ",
    "init:db": "sudo mysql -u root -e \"create database etherscan\" ",
    "update": "node index.js",
    "resave": "node index.js --replay"
  },
  "keywords": [],
  "author": "Web Cerebrium <webcerebrium@gmail.com> (https://webcerebrium.com/)",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "commander": "^2.15.1",
    "cors": "^2.8.4",
    "cross-env": "^5.2.0",
    "debug": "^3.1.0",
    "ethereumjs-util": "^5.2.0",
    "express": "^4.16.3",
    "mysql2": "^1.6.1",
    "web3": "^0.20.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "ganache-cli": "^6.1.6",
    "mocha": "^5.2.0"
  }
}
